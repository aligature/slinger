{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C++ Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/build/slinger",
            "args": ["--username", "aligature", "--source-playlist-url", "v1/users/aligature/starred", "--target-playlist-uri", "6PA9EHuJrASjY5EXh7tDzD", "--filter-months", "24"],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [],
            "externalConsole": true,
            "linux": {
                "MIMode": "gdb",
                "setupCommands": [
                    {
                        "description": "Enable pretty-printing for gdb",
